/*
 * -------------------------------------------------
 *  Nextflow config file for running full-size tests
 * -------------------------------------------------
 * Defines bundled input files and everything required
 * to run a full size pipeline test. Use as follows:
 *   nextflow run nf-core/diaproteomics -profile test_full,<docker/singularity>
 */

params {
  config_profile_name = 'Full test profile'
  config_profile_description = 'Full test dataset to check pipeline function'

  pyprophet_peakgroup_fdr = 1
  pyprophet_peptide_fdr = 1
  pyprophet_protein_fdr = 1
  pyprophet_pi0_start = 0
  pyprophet_pi0_end = 0
  pyprophet_pi0_steps = 0
  DIAlignR_global_align_FDR = 1
  DIAlignR_analyte_FDR = 1
  DIAlignR_unalign_FDR = 1
  DIAlignR_align_FDR = 1
  DIAlignR_query_FDR = 0.5
  generate_spectral_lib = true
  force_option = true

  // Input data
  dda_id = 'https://raw.githubusercontent.com/nf-core/test-datasets/diaproteomics/DDA_test.mzid'
  dda_mzml = 'https://raw.githubusercontent.com/nf-core/test-datasets/diaproteomics/DDA_test.mzML'
  dia_mzmls = ['https://raw.githubusercontent.com/nf-core/test-datasets/diaproteomics/DIA_test_1.mzML','https://raw.githubusercontent.com/nf-core/test-datasets/diaproteomics/DIA_test_2.mzML']
  irts = 'https://raw.githubusercontent.com/nf-core/test-datasets/diaproteomics/irts.pqp'
  swath_windows = 'https://raw.githubusercontent.com/nf-core/test-datasets/diaproteomics/swath_windows.txt'
  unimod = 'https://raw.githubusercontent.com/nf-core/test-datasets/diaproteomics/unimod.xml'
}
